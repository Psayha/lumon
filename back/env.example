# Supabase / PostgreSQL
POSTGRES_USER=postgres
POSTGRES_DB=lumon

# Supabase Keys (получить после первого запуска)
SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_KEY=your-service-key-here

# n8n
N8N_USER=admin
N8N_PASSWORD=lumon_dev
N8N_ENCRYPTION_KEY=lumon-encryption-key-change-in-production

# Важно: настройки зависят от наличия SSL
# Если SSL сертификат УЖЕ получен (HTTPS работает):
N8N_HOST=n8n.psayha.ru
N8N_PROTOCOL=https
N8N_SECURE_COOKIE=true

# Если SSL ЕЩЕ НЕ получен (только HTTP):
# N8N_HOST=n8n.psayha.ru
# N8N_PROTOCOL=http
# N8N_SECURE_COOKIE=false

