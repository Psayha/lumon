        1050,
        500
      ],
      "credentials": {
        "postgres": {
          "id": "supabase-postgres",
          "name": "Supabase PostgreSQL"
        }
      }
    },
    {
      "parameters": {
        "operation": "executeQuery",
        "query": "SELECT COUNT(*) as total\nFROM audit_events\nWHERE ($1::text IS NULL OR action = $1)\n  AND ($2::uuid IS NULL OR user_id = $2);",
        "options": {
          "queryReplacement": "={{ [$json.action || null, $json.userId || null] }}"
        }
      },
      "id": "get-total",
      "name": "Get Total Count",
      "type": "n8n-nodes-base.postgres",
      "typeVersion": 2.4,
      "position": [
        1050,
        600
      ],
      "credentials": {
        "postgres": {
          "id": "supabase-postgres",
          "name": "Supabase PostgreSQL"
        }
      }
    },
    {
      "parameters": {
        "jsCode": "// Format response\nconst logs = $('Get Logs').item.json || [];\nconst total = $('Get Total Count').item.json[0]?.total || 0;\n\nconst formatted = logs.map(log => ({\n  id: log.id,\n  action: log.action,\n  resourceType: log.resource_type,\n  resourceId: log.resource_id,\n  metadata: log.metadata,\n  ip: log.ip,\n  userAgent: log.user_agent,\n  createdAt: log.created_at,\n  user: log.username ? {\n    username: log.username,\n    firstName: log.first_name,\n    lastName: log.last_name\n  } : null\n}));\n\nreturn {\n  json: {\n    success: true,\n    data: formatted,\n    pagination: {\n      total: parseInt(total),\n      limit: $('Extract Data').item.json.limit,\n      offset: $('Extract Data').item.json.offset\n    },\n    traceId: require('crypto').randomUUID()\n  }\n};"
      },
