    {
      "parameters": {
        "operation": "executeQuery",
        "query": "SELECT \n  ae.id,\n  ae.action,\n  ae.resource_type,\n  ae.resource_id,\n  ae.metadata,\n  ae.ip,\n  ae.user_agent,\n  ae.created_at,\n  u.username,\n  u.first_name,\n  u.last_name\nFROM audit_events ae\nLEFT JOIN users u ON ae.user_id = u.id\nWHERE ($1::text IS NULL OR ae.action = $1)\n  AND ($2::uuid IS NULL OR ae.user_id = $2)\nORDER BY ae.created_at DESC\nLIMIT $3 OFFSET $4;",
        "options": {
          "queryReplacement": "={{ [$json.action || null, $json.userId || null, $json.limit, $json.offset] }}"
        }
      },
