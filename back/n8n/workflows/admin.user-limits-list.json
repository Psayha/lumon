    },
    {
      "parameters": {
        "jsCode": "// Format response\nconst limits = $('Get Limits').item.json || [];\n\nconst formatted = limits.map(l => ({\n  id: l.id,\n  userId: l.user_id,\n  limitType: l.limit_type,\n  limitValue: l.limit_value,\n  currentUsage: l.current_usage,\n  resetAt: l.reset_at,\n  createdAt: l.created_at,\n  updatedAt: l.updated_at,\n  user: l.username ? {\n    username: l.username,\n    firstName: l.first_name,\n    lastName: l.last_name\n  } : null\n}));\n\nreturn {\n  json: {\n    success: true,\n    data: formatted,\n    traceId: require('crypto').randomUUID()\n  }\n};"
