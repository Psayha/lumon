    {
      "parameters": {
        "operation": "executeQuery",
        "query": "SELECT \n  ul.id,\n  ul.user_id,\n  ul.limit_type,\n  ul.limit_value,\n  ul.current_usage,\n  ul.reset_at,\n  ul.created_at,\n  ul.updated_at,\n  u.username,\n  u.first_name,\n  u.last_name\nFROM user_limits ul\nLEFT JOIN users u ON ul.user_id = u.id\nWHERE ($1::uuid IS NULL OR ul.user_id = $1)\nORDER BY ul.updated_at DESC;",
        "options": {
          "queryReplacement": "={{ [$json.userId || null] }}"
        }
