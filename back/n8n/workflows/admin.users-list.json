      "parameters": {
        "operation": "executeQuery",
        "query": "SELECT \n  u.id,\n  u.telegram_id,\n  u.username,\n  u.first_name,\n  u.last_name,\n  u.created_at,\n  u.last_login_at,\n  COUNT(DISTINCT c.id) as chats_count,\n  COUNT(DISTINCT uc.company_id) as companies_count,\n  json_agg(DISTINCT jsonb_build_object('company_id', uc.company_id, 'role', uc.role)) FILTER (WHERE uc.company_id IS NOT NULL) as companies\nFROM users u\nLEFT JOIN chats c ON u.id = c.user_id\nLEFT JOIN user_companies uc ON u.id = uc.user_id\nWHERE ($1::text IS NULL OR u.username ILIKE '%' || $1 || '%' OR u.first_name ILIKE '%' || $1 || '%' OR u.last_name ILIKE '%' || $1 || '%')\nGROUP BY u.id, u.telegram_id, u.username, u.first_name, u.last_name, u.created_at, u.last_login_at\nORDER BY u.created_at DESC\nLIMIT $2 OFFSET $3;",
        "options": {
          "queryReplacement": "={{ [$json.search || null, $json.limit, $json.offset] }}"
        }
      },
