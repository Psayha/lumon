    {
      "parameters": {
        "operation": "executeQuery",
        "query": "=SELECT s.id, s.admin_user_id, s.expires_at, s.ip, s.user_agent, u.username, u.is_active\nFROM admin_sessions s\nJOIN admin_users u ON u.id = s.admin_user_id\nWHERE s.token = CAST('{{ $json.token }}' AS UUID)\n  AND s.expires_at > NOW()\n  AND u.is_active = true\nLIMIT 1;",
