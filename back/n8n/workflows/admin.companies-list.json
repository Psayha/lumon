        "postgres": {
          "id": "supabase-postgres",
          "name": "Supabase PostgreSQL"
        }
      }
    },
    {
      "parameters": {
        "jsCode": "// Format response\nconst companies = $('Get Companies').item.json || [];\n\nconst formatted = companies.map(c => ({\n  id: c.id,\n  name: c.name,\n  createdAt: c.created_at,\n  userCount: parseInt(c.user_count) || 0,\n  users: c.users || []\n}));\n\nreturn {\n  json: {\n    success: true,\n    data: formatted,\n    traceId: require('crypto').randomUUID()\n  }\n};"
