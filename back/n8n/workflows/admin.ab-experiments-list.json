    {
      "parameters": {
        "operation": "executeQuery",
        "query": "SELECT \n  e.id,\n  e.name,\n  e.description,\n  e.feature_name,\n  e.enabled,\n  e.traffic_percentage,\n  e.variant_a_config,\n  e.variant_b_config,\n  e.created_at,\n  e.updated_at,\n  COUNT(DISTINCT a.user_id) FILTER (WHERE a.variant = 'A') as variant_a_users,\n  COUNT(DISTINCT a.user_id) FILTER (WHERE a.variant = 'B') as variant_b_users\nFROM ab_experiments e\nLEFT JOIN ab_assignments a ON e.id = a.experiment_id\nGROUP BY e.id, e.name, e.description, e.feature_name, e.enabled, e.traffic_percentage, e.variant_a_config, e.variant_b_config, e.created_at, e.updated_at\nORDER BY e.created_at DESC;",
        "options": {}
      },
      "id": "get-experiments",
      "name": "Get Experiments",
      "type": "n8n-nodes-base.postgres",
      "typeVersion": 2.4,
      "position": [1050, 500],
      "credentials": {
        "postgres": {
          "id": "supabase-postgres",
          "name": "Supabase PostgreSQL"
        }
      }
    },
    {
      "parameters": {
        "jsCode": "// Format response\nconst experiments = $('Get Experiments').item.json || [];\n\nconst formatted = experiments.map(e => ({\n  id: e.id,\n  name: e.name,\n  description: e.description,\n  featureName: e.feature_name,\n  enabled: e.enabled,\n  trafficPercentage: e.traffic_percentage,\n  variantAConfig: e.variant_a_config,\n  variantBConfig: e.variant_b_config,\n  createdAt: e.created_at,\n  updatedAt: e.updated_at,\n  variantAUsers: parseInt(e.variant_a_users) || 0,\n  variantBUsers: parseInt(e.variant_b_users) || 0\n}));\n\nreturn {\n  json: {\n    success: true,\n    data: formatted,\n    traceId: require('crypto').randomUUID()\n  }\n};"
      },
