      "position": [
        850,
        500
      ],
      "continueOnFail": true
    },
    {
      "parameters": {
        "operation": "executeQuery",
        "query": "UPDATE ab_experiments\nSET enabled = COALESCE($1, enabled),\n    traffic_percentage = COALESCE($2, traffic_percentage),\n    variant_a_config = COALESCE($3::jsonb, variant_a_config),\n    variant_b_config = COALESCE($4::jsonb, variant_b_config),\n    updated_at = NOW()\nWHERE id = $5\nRETURNING id, name, enabled, traffic_percentage, variant_a_config, variant_b_config, updated_at;",
        "options": {
          "queryReplacement": "={{ [$json.enabled !== null ? $json.enabled : null, $json.trafficPercentage !== null ? $json.trafficPercentage : null, $json.variantAConfig ? JSON.stringify($json.variantAConfig) : null, $json.variantBConfig ? JSON.stringify($json.variantBConfig) : null, $json.id] }}"
        }
      },
      "id": "update-experiment",
      "name": "Update Experiment",
      "type": "n8n-nodes-base.postgres",
