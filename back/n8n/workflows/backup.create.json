    {
      "parameters": {
        "jsCode": "// Generate backup filename with timestamp\nconst now = new Date();\nconst timestamp = now.toISOString().replace(/[:.]/g, '-').slice(0, -5);\nconst backupFilename = `backup-${timestamp}.sql`;\nconst backupPath = `/var/backups/lumon/${backupFilename}`;\n\nreturn {\n  json: {\n    backupFilename: backupFilename,\n    backupPath: backupPath,\n    timestamp: now.toISOString()\n  }\n};"
      },
      "id": "prepare-backup-data",
      "name": "Prepare Backup Data",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [1450, 600]
    },
    {
      "parameters": {
        "command": "mkdir -p /var/backups/lumon && pg_dump -h supabase-db -U postgres -d lumon -F c -f /var/backups/lumon/{{ $json.backupFilename }}",
        "options": {}
      },
      "id": "create-backup",
      "name": "Create Backup",
      "type": "n8n-nodes-base.executeCommand",
      "typeVersion": 1,
      "position": [1650, 600],
      "continueOnFail": true
    },
    {
      "parameters": {
        "jsCode": "// Manual UUID v4 generator\nfunction generateUUIDv4() {\n  const hex = '0123456789abcdef';\n  let uuid = '';\n  for (let i = 0; i < 36; i++) {\n    if (i === 8 || i === 13 || i === 18 || i === 23) {\n      uuid += '-';\n    } else if (i === 14) {\n      uuid += '4';\n    } else if (i === 19) {\n      uuid += hex[(Math.random() * 4 | 8)];\n    } else {\n      uuid += hex[Math.random() * 16 | 0];\n    }\n  }\n  return uuid;\n}\n\nconst backupData = $('Prepare Backup Data').item.json;\nconst commandResult = $input.item.json;\n\n// Check if backup was created successfully\nconst backupExists = commandResult.stdout && commandResult.stdout.includes('backup') || commandResult.stderr === '';\n\nreturn {\n  json: {\n    success: backupExists,\n    data: {\n      filename: backupData.backupFilename,\n      path: backupData.backupPath,\n      timestamp: backupData.timestamp,\n      size: 0 // Will be updated if file exists\n    },\n    traceId: generateUUIDv4()\n  }\n};"
      },
      "id": "format-response",
      "name": "Build Response",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [1450, 600]
